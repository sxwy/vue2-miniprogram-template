{
  "name": "vue2-miniprogram-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=development UNI_PLATFORM=mp-weixin vue-cli-service uni-build --watch",
    "build": "cross-env NODE_ENV=production UNI_PLATFORM=mp-weixin vue-cli-service uni-build",
    "serve:mock": "cross-env VUE_APP_ENV=mock yarn run serve",
    "serve:alpha": "cross-env VUE_APP_ENV=alpha yarn run serve",
    "serve:rc": "cross-env VUE_APP_ENV=rc yarn run serve",
    "serve:prod": "cross-env VUE_APP_ENV=prod yarn run serve",
    "build:mock": "cross-env VUE_APP_ENV=mock yarn run build",
    "build:alpha": "cross-env VUE_APP_ENV=alpha yarn run build",
    "build:rc": "cross-env VUE_APP_ENV=rc yarn run build",
    "build:prod": "cross-env VUE_APP_ENV=prod yarn run build",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:js": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./src",
    "lint:css": "stylelint ./src/**.{vue,css,less,sass,scss}",
    "format": "yarn run format:js && yarn run format:css",
    "format:js": "eslint --fix --ext .vue,.js,.jsx,.ts,.tsx ./src",
    "format:css": "stylelint --fix --allow-empty-input ./src/**.{vue,css,less,sass,scss}",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
