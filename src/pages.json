{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
    }
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "vue2-miniprogram-template",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "pages": [
    {
      "path": "pages/launch/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/launch/login-phone-auth",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/launch/login-account-password",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/webview/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/authorization/index",
      "style": {
        "navigationStyle": "custom"
      }
    }
  ],
  "subPackages": [
    {
      "root": "packages/home",
      "pages": [
        {
          "path": "index"
        }
      ]
    }
  ]
}
